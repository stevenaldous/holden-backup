function initVimeo(){
  //YouTube case handled in separate file
  var url = "{{ settings.hero_video_link }}";
  var videoSubstring = url.match(/\d+/)[0];

  if (url.indexOf("vimeo") != -1){
    createIframe(videoSubstring);
  } else {
    //Youtube -- hide poster image and show video
    $('#iframe-wrapper').load(function() {
      $('.video').css('background-image','none');
      $('#iframe-wrapper').css('display','block');
    });
  }


  function createIframe(substring){
    var iframe = "<iframe sandbox='allow-same-origin allow-scripts allow-popups' wmode='transparent' id='vimeo_player' src='//player.vimeo.com/video/"+substring+"?api=1&player_id=vimeo_player&autoplay=1&loop=1' width='' height='' frameborder='0' webkitAllowFullScreen mozallowfullscreen allowFullScreen ></iframe>";
   $('#iframe-wrapper').append( $( iframe) );
    positionHeroVideo();
    $(window).on("resize",function(e){
      positionHeroVideo();
    });
  }

  function positionHeroVideo(){
    var ratio = {{settings.hero_video_ratio}};
    var percent = 1/ratio;
    var videoWidth = $('#video').width();
    var initialHeight = videoWidth*(9/16);
    //set iframe dimensions
    $('#vimeo_player').attr({"width":videoWidth,"height":initialHeight});
    $(".sidedock,.controls").remove();
    //hack for videos not uploaded under Vimeo Plus accounts
    var vimeo_iframe = $('#vimeo_player')[0];
    var player = $f(vimeo_iframe);
	
    {% unless settings.enable_sound %}
    player.addEvent('ready', function() {
        player.api('setVolume', 0);
    });
    {% endunless %}
    
    //set wrapper height
    if (ratio != 4/3){
      var videoHeight = videoWidth*percent - 10; //10px for good measure
      $('#video').css({"height":videoHeight});
      //set margin-top for vertical align middle
      var heightOffset = (initialHeight - videoHeight)/2;
      $('#vimeo_player').css('margin-top',-heightOffset);
    }
    //show it once it's calculated
    $('#video').css('opacity','1');
  }

  $('#vimeo_player').load(function() {
    $('.video').css('background-image','none');
    $(this).css('display','block');
  });

  };
};

