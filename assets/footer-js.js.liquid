PaloAlto.initStickyNav = function() {
  var scrolled = 0;
  $(window).on("resize load",function(e){
    var headerHeight = $(".site-header").outerHeight();
    $('.header-offset').css('height',headerHeight);

    //Swap in ghost element and make it sticky
    if (scrolled == 0){
