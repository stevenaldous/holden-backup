  letter-spacing: 2px;
  border: 2px solid $colorBtnBorder;
  font-family: $buttonfont;
  transition: all 0.2s ease;
  line-height: 1.5;

  /*================ Set primary button colors ================*/
  &:hover,
  &:active,
  &:focus {
      background: darken($colorBtnPrimary,10%);
      border: 2px solid darken($colorBtnBorder,10%);
      color: darken($colorBtnText,10%);
